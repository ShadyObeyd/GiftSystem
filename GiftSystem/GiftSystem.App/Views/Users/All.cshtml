@model AllUsersViewModel

<table class="table">
    <thead>
        <tr>
            <th scope="col">Username</th>
            <th scope="col">Credits in account</th>
            <th scope="col">Sent Transactions</th>
            <th scope="col">Received Transactions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Credits</td>
                <td>@user.SentTransactionsCount</td>
                <td>@user.ReceivedTransactionsCount</td>
            </tr>
        }
    </tbody>
</table>